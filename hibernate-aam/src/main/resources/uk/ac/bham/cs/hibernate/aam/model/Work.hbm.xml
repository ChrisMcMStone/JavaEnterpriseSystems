<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="uk.ac.bham.cs.aam.model.impl.WorkImpl" table="work">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="version" type="java.lang.Integer">
			<column name="version" />
		</property>
		<many-to-one name="customer" class="uk.ac.bham.cs.aam.model.Customer">
			<column name="customer_id" />
		</many-to-one>
		<property name="startDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentLocalDate">
			<column name="start_date" />
		</property>
		<property name="completionDate"
			type="org.jadira.usertype.dateandtime.joda.PersistentLocalDate">
			<column name="completion_date" />
		</property>
		<many-to-one name="asset" class="uk.ac.bham.cs.aam.model.Asset">
			<column name="asset_id" />
		</many-to-one>
	</class>
</hibernate-mapping>
